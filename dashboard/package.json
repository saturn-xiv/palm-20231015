    "emoji-mart": "^3.0.1",
    "google-map-react": "^2.1.10",
    "google-protobuf": "^3.20.1-rc.1",
    "grpc-web": "^1.3.1",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "qrcode.react": "^3.0.1",
    "react": "17.x",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "17.x",
    "react-dropzone": "^12.0.5",
