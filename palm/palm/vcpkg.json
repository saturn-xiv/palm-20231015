{
  "name": "palm",
  "version": "2023.5.4",
  "dependencies": [
    "openssl",
    "boost-algorithm",
    { "name": "boost-asio", "features": ["ssl"] },
    "boost-date-time",
    "boost-detail",
    "boost-exception",
    "boost-filesystem",
    "boost-json",
    "boost-locale",
    "boost-log",
    "boost-math",
    "boost-program-options",
    "boost-property-tree",
    "boost-random",
    "boost-range",
    "boost-regex",
    "boost-system",
    "boost-test",
    "boost-thread",
    "boost-timer",
    "boost-uuid",

    "libevent",
    "liblzma",
    { "name": "sail", "default-features": false, "features": ["png"] },

    { "name": "paho-mqttpp3", "features": ["ssl"] },
    "librabbitmq",

    { "name": "cpr", "features": ["ssl"] },
    { "name": "aws-sdk-cpp", "features": ["s3", "sqs"] },
    { "name": "libssh2", "features": ["zlib"] },
    "minio-cpp",
    "cserialport",

    "cppcodec",
    "pugixml",
    "tomlplusplus",
    "yaml-cpp",
    "msgpack-c",
    { "name": "lua", "features": ["cpp"] },

    { "name": "nlohmann-json", "features": ["diagnostics"] },
    "mstch",
    "inja",
    "cpp-httplib",
    { "name": "poco", "default-features": false, "features": ["crypto"] },
    "jwt-cpp",

    { "name": "libpq", "features": ["nls", "openssl", "zlib"] },
    "libpqxx",
    { "name": "libmariadb", "features": ["openssl", "zlib"] },
    { "name": "sqlite3", "features": ["math", "zlib"] },
    { "name": "soci", "features": ["boost", "sqlite3", "postgresql"] },
    { "name": "redis-plus-plus", "features": ["cxx17"] },
    { "name": "leveldb", "features": ["crc32c"] },

    "flatbuffers",
    "protobuf",
    { "name": "abseil", "features": ["cxx17"] },
    "re2",
    "grpc",

    "spdlog",
    "catch2",
    "argparse",
    "magic-enum",
    "mailio"
  ]
